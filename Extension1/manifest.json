 {
    "name": "UI Analyzer",
    "version": "2.0",
    "description": "Neophytes smart UI Analyzer",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_icon": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
	"content_scripts": [
    {
      "matches": ["http://localhost:9182/*"],
      "js": ["jquery-3.5.1.min.js","FileSaver.js","jquery.wordexport.js"],
	  "css" : ["style.css"]
    }
	],
	"options_page": "options.html",
    "manifest_version": 2,
	"web_accessible_resources": [ "tray.html","config.json","FileSaver.js","jquery.wordexport.js"]
  }